        <Image Margin="0,75,400,125" OpacityMask="#FFB4B4B4" Source="/Eventlink2023_sort-logo-transparent-bakgrunn_utenkontaktinfo-og-slagord.png" />

        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-60,-420,0">
            <TextBlock Text="Eventlink AS" FontSize="24" FontWeight="Bold"/>
            <TextBlock Text="Lisens: EVPOS-2024-0098" Margin="0,6,0,0"/>
            <TextBlock Text="Storgata 12" Margin="0,6,0,0"/>
            <TextBlock Text="0184 Oslo" Margin="0,6,0,0"/>
